#pragma config(Hubs,  S1, HTMotor,  none,     none,     none)
#pragma config(Sensor, S1,     ,               sensorI2CMuxController)
#pragma config(Motor,  mtr_S1_C1_1,     leftM,         tmotorTetrix, PIDControl, reversed, driveLeft, encoder)
#pragma config(Motor,  mtr_S1_C1_2,     rightM,        tmotorTetrix, PIDControl, driveRight, encoder)
//*!!Code automatically generated by 'ROBOTC' configuration wizard               !!*//

task main()
{
setMultipleMotors(64,leftM,rightM);
stopAllMotors();
wait10Msec(100);
setMultipleMotors(-64,leftM,rightm,);
stopALLMotors();
wait10Msec(100);
motor[rightM]=-64;
motor[leftM]=64;
stopAllMotors();
wait10Msec(100);
motor[leftM]=-64;
motor[rightM]=64;
stopAllMotors();
}
